#include <iostream>
using namespace std;

// Calculates the Greatest Common Divisor using the Euclidean algorithm
int gcd(int a, int b) {
    if (b == 0) return a;
    return gcd(b, a % b);
}

int main() {
    int t, a, b, c;
    cin >> t;
    while (t--) {
        cin >> a >> b >> c;
        // The specific measure is possible iff c is a multiple of gcd(a,b) and c <= max(a,b)
        if (c % gcd(a, b) == 0 && c <= max(a, b)) {
            cout << "YES" << endl;
        } else {
            cout << "NO" << endl;
        }
    }
    return 0;
}

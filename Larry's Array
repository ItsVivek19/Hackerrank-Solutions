#include <iostream>
#include <vector>
using namespace std;

string larrysArray(vector<int> &arr) {
    int invCount = 0;
    int n = arr.size();
    for(int i = 0; i < n; i++) {
        for(int j = i + 1; j < n; j++) {
            if(arr[i] > arr[j]) invCount++;
        }
    }
    return (invCount % 2 == 0) ? "YES" : "NO";
}

int main() {
    int t; 
    cin >> t;
    while(t--) {
        int n; 
        cin >> n;
        vector<int> arr(n);
        for(int i = 0; i < n; i++) {
            cin >> arr[i];
        }
        cout << larrysArray(arr) << "\n";
    }
    return 0;
}

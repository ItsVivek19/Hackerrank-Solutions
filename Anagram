#include <bits/stdc++.h>
using namespace std;

int anagram(string s) {
    int n = s.size();
    
    // If length is odd, cannot split equally
    if (n % 2 == 1) return -1;
    
    string a = s.substr(0, n/2);
    string b = s.substr(n/2);

    vector<int> freq(26, 0);

    // Count frequencies for first half
    for (char c : a) {
        freq[c - 'a']++;
    }
    // Subtract frequencies for second half
    for (char c : b) {
        freq[c - 'a']--;
    }

    // Count how many extra characters in first half
    int changes = 0;
    for (int x : freq) {
        if (x > 0) changes += x;
    }

    return changes;
}

int main() {
    int t;
    cin >> t;
    while (t--) {
        string s;
        cin >> s;
        cout << anagram(s) << endl;
    }
    return 0;
}

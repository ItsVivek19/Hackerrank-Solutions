#include <iostream>
#include <vector>
using namespace std;

int sansaXor(const vector<int>& arr) {
    int n = arr.size();
    if (n % 2 == 0) return 0; // Even length => XOR cancels out
    int result = 0;
    // XOR elements at even indices (0-based)
    for (int i = 0; i < n; i += 2) {
        result ^= arr[i];
    }
    return result;
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    
    int t;
    cin >> t;
    while (t--) {
        int n; 
        cin >> n;
        vector<int> arr(n);
        for (int i = 0; i < n; ++i) {
            cin >> arr[i];
        }
        cout << sansaXor(arr) << "\n";
    }
    return 0;
}

#include <stdio.h>

void stones(int n, int a, int b) {
    if (a > b) {
        int temp = a;
        a = b;
        b = temp;
    }

    int diff = b - a;
    for (int i = 0; i < n; i++) {
        int value = a * (n - 1 - i) + b * i;
        printf("%d", value);
        if (diff != 0 && i != n - 1) {
            printf(" ");
        }
    }
    printf("\n");
}

int main() {
    int T;
    scanf("%d", &T);

    while (T--) {
        int n, a, b;
        scanf("%d %d %d", &n, &a, &b);
        stones(n, a, b);
    }

    return 0;
}
